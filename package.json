{
  "name": "first-step-staffing",
  "description": "this is a description",
  "version": "1.0.0",
  "cacheDirectories": [
    "client/node_modules",
    "server/node_modules"
  ],
  "scripts": {
    "start": "cd server/ && npm start",
    "start-backend": "cd server/ && npm run dev",
    "state-frontend": "cd client && npm start",
    "dev": "npm run start-backend && npm run start-frontend",
    "install-client": "npm install --prefix client",
    "install-server": "npm install --prefix server",
    "build": "npm run build-client && npm run build-server",
    "build-client": "cd client/ && npm run build",
    "build-server": "cd server/ && npm run build",
    "postinstall": "npm run install-client && npm run install-server && npm run build"
  },
  "engines": {
    "npm": "6.6.0",
    "node": "10.10.0"
  },
  "dependencies": {
    "carbon-components": "^10.7.1",
    "carbon-components-react": "^7.7.1",
    "carbon-icons": "^7.0.7",
    "react-signature-pad-wrapper": "^1.2.6",
    "signature_pad": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-prettier": "^3.1.0"
  }
}
