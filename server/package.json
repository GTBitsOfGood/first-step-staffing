{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./build/bin/www",
    "debugger": "nodemon --inspect --exec babel-node ./bin/www",
    "dev": "nodemon --exec babel-node ./bin/www",
    "build": "babel . --ignore node_modules --out-dir build && cp -R ./bin ./build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "apollo-server": "^2.8.1",
    "apollo-server-express": "^2.8.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "dotenv-cli": "^1.4.0",
    "express": "~4.16.0",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.3.1"
  }
}
